{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 500,
  "title": "Airports in Australia",
  "projection": {"type": "equalEarth", "center": [135, -28], "scale": 700},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/jale0023-gif/3179/refs/heads/main/ne_110m.json",
        "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/jale0023-gif/3179/refs/heads/main/australia_airports.csv"
      },
      "mark": {"type": "circle", "color": "red", "opacity": 0.8, "size": 80},
      "encoding": {
        "longitude": {"field": "Longitude_dd", "type": "quantitative"},
        "latitude": {"field": "Latitude_dd", "type": "quantitative"},
        "tooltip": [
          {"field": "Airport Name", "type": "nominal", "title": "Airport"},
          {"field": "City", "type": "nominal", "title": "City"},
          {"field": "State", "type": "nominal", "title": "State"},
          {"field": "Code", "type": "nominal", "title": "Code"}
        ]
      }
    }
  ]
}



